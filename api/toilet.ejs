        level: 10 //지도의 레벨(확대, 축소 정도)
      };

      if (navigator.geolocation) navigator.geolocation.getCurrentPosition((potition) => {
          options.center = new kakao.maps.LatLng(potition.coords.latitude, potition.coords.longitude)
        })

      var imageSrc = 'https://image.flaticon.com/icons/png/512/195/195439.png', // 마커이미지의 주소입니다    
          imageSize = new kakao.maps.Size(30, 30), // 마커이미지의 크기입니다
          imageOption = {offset: new kakao.maps.Point(27, 69)}; // 마커이미지의 옵션입니다. 마커의 좌표와 일치시킬 이미지 안에서의 좌표를 설정합니다.
            
      // 마커의 이미지정보를 가지고 있는 마커이미지를 생성합니다
      var markerImage = new kakao.maps.MarkerImage(imageSrc, imageSize, imageOption)

      var map = new kakao.maps.Map(container, options); //지도 생성 및 객체 리턴
      data.forEach((d) => {
        const mark = new kakao.maps.Marker({ position: new kakao.maps.LatLng(d.위도, d.경도), image: markerImage })
        mark.setMap(map)

        kakao.maps.event.addListener(mark, 'click', function() {
          const infowindow = new kakao.maps.InfoWindow({ position: new kakao.maps.LatLng(d.위도, d.경도), content: `<div style="height: 100px; width: 300px; padding: 20px; word-break: break-all: word-wrap: break-word;"><i class="fas fa-info-circle"></i> 설치 년도: ${d.설치년도|| '정보 없음'}<br />주소: ${d.소재지도로명주소|| '정보 없음'}<br />남녀 공용화장실 여/부: ${d.남녀공용화장실여부 === 'N' ? '아니요' : '네'|| '정보 없음'}</div>` })
          infowindow.open(map, mark)
          setTimeout(() => {
            infowindow.close()
          }, 5000)
        })
      })


